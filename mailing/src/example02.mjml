<mjml>
  <mj-head>
    <!-- TITLE -->
    <mj-title>Title goes here</mj-title>

    <!-- PREVIEW -->
    <mj-preview>Preview goes here</mj-preview>

    <!-- FONTS -->
    <mj-style>
      @font-face {
        font-family: 'Inter';
        font-style: normal;
        font-weight: 400;
        font-display: swap;
        src: url(https://fonts.gstatic.com/s/inter/v12/UcC73FwrK3iLTeHuS_fvQtMwCp50KnMa1ZL7W0Q5nw.woff2) format('woff2');
        unicode-range: U+0000-00FF, U+0131, U+0152-0153, U+02BB-02BC, U+02C6, U+02DA, U+02DC, U+2000-206F, U+2074, U+20AC, U+2122, U+2191, U+2193, U+2212, U+2215, U+FEFF, U+FFFD;
      }

      @font-face {
        font-family: 'Inter';
        font-style: normal;
        font-weight: 500;
        font-display: swap;
        src: url(https://fonts.gstatic.com/s/inter/v12/UcC73FwrK3iLTeHuS_fvQtMwCp50KnMa1ZL7W0Q5nw.woff2) format('woff2');
        unicode-range: U+0000-00FF, U+0131, U+0152-0153, U+02BB-02BC, U+02C6, U+02DA, U+02DC, U+2000-206F, U+2074, U+20AC, U+2122, U+2191, U+2193, U+2212, U+2215, U+FEFF, U+FFFD;
      }

      @font-face {
        font-family: 'JetBrains Mono';
        font-style: normal;
        font-weight: 400;
        font-display: swap;
        src: url(https://fonts.gstatic.com/s/jetbrainsmono/v17/tDbv2o-flEEny0FZhsfKu5WU4zr3E_BX0PnT8RD8yKwBNntkaToggR7BYRbKPxDcwgknk-4.woff2) format('woff2');
        unicode-range: U+0000-00FF, U+0131, U+0152-0153, U+02BB-02BC, U+02C6, U+02DA, U+02DC, U+2000-206F, U+2074, U+20AC, U+2122, U+2191, U+2193, U+2212, U+2215, U+FEFF, U+FFFD;
      }

      @font-face {
        font-family: 'JetBrains Mono';
        font-style: normal;
        font-weight: 500;
        font-display: swap;
        src: url(https://fonts.gstatic.com/s/jetbrainsmono/v17/tDbv2o-flEEny0FZhsfKu5WU4zr3E_BX0PnT8RD8yKwBNntkaToggR7BYRbKPxDcwgknk-4.woff2) format('woff2');
        unicode-range: U+0000-00FF, U+0131, U+0152-0153, U+02BB-02BC, U+02C6, U+02DA, U+02DC, U+2000-206F, U+2074, U+20AC, U+2122, U+2191, U+2193, U+2212, U+2215, U+FEFF, U+FFFD;
      }
    </mj-style>


    <!-- BREAKPOINT -->
    <mj-breakpoint width="484px" />

    <mj-attributes>
      <!-- NORMALIZE -->
      <mj-all padding="0" margin="0" font-family="Inter, Helvetica, Arial, sans-serif" color="#344461" />
      <mj-body background-color="#E5E9EC" width="636px" />
      <mj-section full-width="full-width" />
      <mj-text font-family="Inter, sans-serif" />
      <mj-divider border-width="1px" border-style="solid" border-color="#D4D9DF" width="100%" />

      <!-- SPACING -->
      <mj-class name="fds-spacing--16" height="16px" />
      <mj-class name="fds-spacing--24" height="24px" />
      <mj-class name="fds-spacing--32" height="32px" />

      <!-- HIGHLIGHT -->
      <mj-class name="fds-highlight" background-color="#E5E9EC" padding="16px 24px" border-radius="8px" color="#344461" />
    </mj-attributes>

    <mj-style>
      img {
        object-fit: cover !important;
      }

      .fds-email-heading-01 div {
        font-size: 22px !important;
        line-height: 26px !important;
        font-weight: 500;
        letter-spacing: -0.32px;
        color: #121D31 !important;
      }

      .fds-email-body-01 div {
        font-size: 15px !important;
        line-height: 24px !important;
        font-weight: 400;
      }

      .fds-email-label-01 div {
        font-size: 15px !important;
        line-height: 18px !important;
        font-weight: 500;
      }

      .fds-email-label-02 div {
        font-size: 12px !important;
        line-height: 18px !important;
        font-weight: 400;
        letter-spacing: 0.32px;
      }

      .fds-email-label-03 div {
        font-size: 11px !important;
        line-height: 14px !important;
        font-weight: 400;
        letter-spacing: 0.32px;
      }

      .fds-email-amount-01 div {
        font-family: 'JetBrains Mono', monospace !important;
        font-size: 18px !important;
        line-height: 24px !important;
        font-weight: 500;
      }

      .fds-email-amount-02 div {
        font-family: 'JetBrains Mono', monospace !important;
        font-size: 15px !important;
        line-height: 24px !important;
        font-weight: 400;
      }

      .fds-text-link {
        text-align: left !important;
        color: #344461 !important;
        text-decoration: underline;
      }

      .fds-footer-link {
        font-family: Inter, sans-serif;
        font-size: 12px;
        line-height: 18px;
        font-weight: 400;
        letter-spacing: 0.32px;
        text-align: left !important;
        color: #505F79 !important;
        text-decoration: none;
      }

      .fds-footer-link:hover {
        text-decoration: underline;
      }

      .fds-table th,
      .fds-table td {
        font-size: 15px !important;
        line-height: 24px !important;
      }

      .fds-table th {
        font-weight: 500 !important;
      }

      .fds-table td {
        font-weight: 400 !important;
      }

      .fds-table.fds-table--truncate td {
        overflow: hidden !important;
        white-space: nowrap !important;
        text-overflow: ellipsis !important;
        max-width: 0 !important;
      }

      .fds-inline-block {
        display: inline-block !important;
      }

      .fds-u-pt-4 {
        padding-top: 4px !important;
      }

      @media only screen and (max-width: 484px) {
        .fds-email-heading-01 div {
          font-size: 18px !important;
          line-height: 18px !important;
        }

        .fds-email-body-01 div {
          font-size: 13px !important;
          line-height: 22px !important;
        }

        .fds-email-label-01 div {
          font-size: 13px !important;
          line-height: 22px !important;
        }

        .fds-email-label-02 div {
          font-size: 11px !important;
          line-height: 16px !important;
        }

        .fds-email-label-03 div {
          font-size: 10px !important;
          line-height: 12px !important;
        }

        .fds-email-amount-01 {
          font-size: 16px !important;
          line-height: 24px !important;
        }

        .fds-email-amount-02 div {
          font-size: 13px !important;
          line-height: 22px !important;
        }
      }
    </mj-style>
  </mj-head>

  <mj-body>
    <mj-wrapper padding="80px 16px">
      <!-- START HEADER -->
      <mj-section background-color="#D4D9DF" border-radius="8px 8px 0 0" padding="20px 24px">
        <mj-column width="60%" padding="8px">
          <mj-image align="left" width="150px" src="https://parasut-mailing.s3.amazonaws.com/parasut-logo-2x.png" alt="brand_logo" href="#" />
        </mj-column>
        <mj-column width="40%" padding="8px">
          <mj-text css-class="fds-email-label-03" color="#505F79" align="left">
            <span class="fds-inline-block">Şirket adı: X Yazılım teknolojileri A.Ş</span>
            <span class="fds-inline-block fds-u-pt-4">Şube: Sarıyer, Istanbul</span>
            <span class="fds-inline-block fds-u-pt-4">Hesap ID: 234323432</span>
          </mj-text>
        </mj-column>
      </mj-section>
      <!-- END HEADER -->

      <!-- START CONTENT -->
      <mj-section padding="32px" background-color="#ffffff">
        <mj-column width="100%">
          <mj-text css-class="fds-email-heading-01" color="#121D31">Kritik stokta ürünleriniz var.</mj-text>

          <mj-spacer mj-class="fds-spacing--32" />

          <mj-text css-class="fds-email-body-01">Son gerçekleştirdiğiniz stok çıkışı sonucunda aşağıdaki ürünleriniz kritik stok seviyesinin altına düşmüştür.</mj-text>

          <mj-spacer mj-class="fds-spacing--32" />

          <mj-table css-class="fds-table fds-table--truncate" table-layout="auto" container-background-color="#ffffff" padding="0">
            <tr>
              <th width="25%" align="left" style="padding: 8px; border: 2px solid #D4D9DF; text-align: left !important">Kodu</th>
              <th width="25%" align="center" style="padding: 8px; border: 2px solid #D4D9DF; text-align: center !important;">Adı</th>
              <th width="25%" align="center" style="padding: 8px; border: 2px solid #D4D9DF; text-align: center !important;">Kritik seviye</th>
              <th width="25%" align="right" style="padding: 8px; border: 2px solid #D4D9DF; text-align: right !important;">Mevcut stok</th>
            </tr>
            <tr>
              <td width="25%" align="left" style="padding: 8px; border: 2px solid #D4D9DF; text-align: left !important">1234567</td>
              <td width="25%" align="center" style="padding: 8px; border: 2px solid #D4D9DF; text-align: center !important;">This is a Kısa Kollu Tişört</td>
              <td width="25%" align="center" style="padding: 8px; border: 2px solid #D4D9DF; font-family: 'JetBrains Mono', monospace !important; text-align: center !important;">10,00</td>
              <td width="25%" align="right" style="padding: 8px; border: 2px solid #D4D9DF; font-family: 'JetBrains Mono', monospace !important; color: #DA1E28 !important; text-align: right !important;">5,00</td>
            </tr>
            <tr>
              <td width="25%" align="left" style="padding: 8px; border: 2px solid #D4D9DF; text-align: left !important">1234568</td>
              <td width="25%" align="center" style="padding: 8px; border: 2px solid #D4D9DF; text-align: center !important;">Taşlı pantolon</td>
              <td width="25%" align="center" style="padding: 8px; border: 2px solid #D4D9DF; font-family: 'JetBrains Mono', monospace !important; text-align: center !important;">8,00</td>
              <td width="25%" align="right" style="padding: 8px; border: 2px solid #D4D9DF; font-family: 'JetBrains Mono', monospace !important; color: #DA1E28 !important; text-align: right !important;">2,00</td>
            </tr>
          </mj-table>

          <mj-spacer mj-class="fds-spacing--32" />

          <mj-button href="#" css-class="fds-btn--primary" font-family="Inter, sans-serif" font-size="15px" line-height="16px" font-weight="500" color="#ffffff" background-color="#0F62FE" container-background-color="#ffffff" inner-padding="16px" border-radius="4px" align="left">Hizmet ve ürünlere git</mj-button>
        </mj-column>
      </mj-section>

      <mj-section padding="32px" padding-top="0" background-color="#ffffff">
        <mj-column width="50%" padding="24px" background-color="#F7F9FA">
          <mj-text css-class="fds-email-label-01">This is a promotional section header</mj-text>
          <mj-text css-class="fds-email-body-01" padding-top="8px">Lorem ipsum dolor sit amet, consectetur adipiscing elit.</mj-text>
          <mj-button href="#" css-class="fds-btn--secondary" font-family="Inter, sans-serif" font-size="15px" line-height="16px" font-weight="500" color="#ffffff" background-color="#505F79" container-background-color="transparent" inner-padding="8px 16px" border-radius="4px" align="left" padding-top="16px">Secondary Button</mj-button>
        </mj-column>

        <mj-column width="50%" padding="24px" background-color="#F7F9FA">
          <mj-image fluid-on-mobile="true" height="140px" border-radius="8px" src="https://mcusercontent.com/542129b0d23244e20a9c0dd7e/images/82c1b653-e216-56b8-0db4-45f40485956a.jpg" />
        </mj-column>
      </mj-section>

      <mj-section padding="32px" background-color="#ffffff" border-radius="0 0 8px 8px">
        <mj-column width="100%">
          <mj-image fluid-on-mobile="true" width="540px" height="152px" border-radius="8px" src="https://mcusercontent.com/542129b0d23244e20a9c0dd7e/images/39abec24-d568-10d8-c5ce-d349a1d4e66c.png" />
        </mj-column>
      </mj-section>
      <!-- END CONTENT -->

      <!-- START FOOTER -->
      <mj-section padding="32px" background-color="transparent">
        <mj-column width="100%">
          <mj-text css-class="fds-email-label-02" color="#505F79">Company Name kullanıcısı olduğunuz için bu iletiyi alıyorsunuz.</mj-text>

          <mj-text color="#505F79" padding-top="8px">
