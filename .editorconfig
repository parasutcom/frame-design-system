# EditorConfig, aynı proje üzerinde çalışan geliştiricilerin kullandıkları,
# farklı editörler ve IDE'ler arasındaki kodlama stilini belirtir.
# editorconfig.org


root = true

[*]
charset = utf-8
indent_size = 2
indent_style = space
insert_final_newline = true
max_line_length = 80
trim_trailing_whitespace = true
